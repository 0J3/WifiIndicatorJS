{
	"name": "wifiindicatorjs",
	"description": "wifi ping thing cuz mine broke",
	"version": "1.0.0",
	"main": "js/index.js",
	"repository": "https://github.com/0J3/WifiIndicatorJS.git",
	"author": "0J3 <git@wafflepowered.com>",
	"license": "MIT",
	"dependencies": {
		"chalk": "^4.1.0",
		"ping": "^0.4.0"
	},
	"devDependencies": {
		"electron": "^11.0.2",
		"electron-builder": "^22.9.1"
	},
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --win",
		"postinstall": "electron-builder install-app-deps"
	},
	"build": {
		"appId": "0j3.js.wifiindicator",
		"nsis": {
			"oneClick": false,
			"allowElevation": true,
			"perMachine": true,
			"allowToChangeInstallationDirectory": true,
			"installerIcon": "Icons/Icon.png",
			"uninstallerIcon": "Icons/Icon.png",
			"license": "license.html"
		}
	}
}
